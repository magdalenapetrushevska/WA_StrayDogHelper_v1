@model IEnumerable<WA_StrayDogHelper_v1.Models.IdentityModels.ApplicationUser>



<h1>List of contacts</h1>
<br />

<table class="table table-hover">
    <thead class="thead-dark">
        <tr>
           
            <th>
                User
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                
                <td>
                    <b>@Html.DisplayFor(modelItem => item.Email)</b>
                </td>
                <td>
                    <a asp-action="VidiRazgovor" asp-route-id="@item.Id" class="btn btn-primary">See conversation</a>
                </td>
            </tr>
        }
    </tbody>
</table>
